        <boolProp name="resetInterpreter">false</boolProp>
        <stringProp name="script">import java.util.Date;
import java.text.SimpleDateFormat;

// 获取当天起始时间、结束时间
SimpleDateFormat dateFormat = new SimpleDateFormat(&quot;yyyy-MM-dd&quot;);
String startTimeToDay = dateFormat.format(new Date()) + &quot; 00:00:00&quot;;
String endTimeToDay = dateFormat.format(new Date()) + &quot; 23:59:59&quot;;
vars.put(&quot;startTimeToDay&quot;,startTimeToDay);
vars.put(&quot;endTimeToDay&quot;,endTimeToDay);

// 获取当月起始日期
Calendar c = Calendar.getInstance();   
c.add(Calendar.MONTH, 0);
c.set(Calendar.HOUR_OF_DAY,0);
c.set(Calendar.MINUTE,0);
c.set(Calendar.SECOND,0);
c.set(Calendar.DAY_OF_MONTH,1);//设置为1号,当前日期既为本月第一天 
String startSameMonth = dateFormat.format(c.getTime()) + &quot; 00:00:00&quot;;
vars.put(&quot;startSameMonth&quot;,startSameMonth);

// 获取当月结束日期
Calendar ca = Calendar.getInstance();    
ca.set(Calendar.DAY_OF_MONTH, ca.getActualMaximum(Calendar.DAY_OF_MONTH));  
ca.set(Calendar.HOUR_OF_DAY,23);
ca.set(Calendar.MINUTE,59);
ca.set(Calendar.SECOND,59);
String endSameMonth = dateFormat.format(ca.getTime()) + &quot; 23:59:59&quot;;
vars.put(&quot;endSameMonth&quot;,endSameMonth);

// 获取上月的第一天
Calendar cal = Calendar.getInstance();
cal.add(Calendar.MONTH, -1);
cal.set(Calendar.DAY_OF_MONTH, 1);
cal.set(Calendar.HOUR_OF_DAY, 0);
cal.set(Calendar.MINUTE, 0);
cal.set(Calendar.SECOND, 0);
cal.set(Calendar.MILLISECOND, 0);
String last_month = dateFormat.format(cal.getTime()) + &quot; 00:00:00&quot;;
vars.put(&quot;lastMonth&quot;,last_month);



// 获取上一个月的中旬
// 使用Calendar类计算上个月的日期
Calendar calendar = Calendar.getInstance();
calendar.setTimeInMillis(System.currentTimeMillis());
calendar.add(Calendar.MONTH, -1); // 减去1个月
calendar.set(Calendar.DATE, 15); // 设置为上个月的15号
// 格式化日期并输出
String middleDate = dateFormat.format(calendar.getTime())+ &quot; 23:59:59&quot;;
vars.put(&quot;middleDate&quot;,middleDate);

// 获取下一个月
Calendar cd = Calendar.getInstance();
cd.add(Calendar.MONTH, 1);
String nextMonthDate = dateFormat.format(cd.getTime());
vars.put(&quot;nextMonth&quot;,nextMonthDate);
// 获取下2个月
Calendar cdl = Calendar.getInstance();
cdl.add(Calendar.MONTH, 2);
cdl.set(Calendar.DATE, 20); 
String nexTowtMonthDate = dateFormat.format(cdl.getTime());
vars.put(&quot;nexTowtMonthDate&quot;,nexTowtMonthDate);





</stringProp>
